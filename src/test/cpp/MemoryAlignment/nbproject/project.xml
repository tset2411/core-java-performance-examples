<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.cnd.makeproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/make-project/1">
            <name>MemoryAlignment</name>
            <c-extensions/>
            <cpp-extensions>cpp</cpp-extensions>
            <header-extensions/>
            <make-dep-projects/>
            <sourceRootList/>
            <confList>
                <confElem>
                    <name>Debug</name>
                    <type>1</type>
                </confElem>
                <confElem>
                    <name>Release</name>
                    <type>1</type>
                </confElem>
            </confList>
            <sourceEncoding>UTF-8</sourceEncoding>
        </data>
    </configuration>
</project>
